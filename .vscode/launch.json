{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Server (development)",
            "cwd": "${workspaceRoot}/packages/qg_server_starter",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceRoot}/packages/qg_server_starter/bin/bootstrap/main_development.dart"
        },
        {
            "name": "Launch Server (staging)",
            "cwd": "${workspaceRoot}/packages/qg_server_starter",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceRoot}/packages/qg_server_starter/bin/server.dart"
        },
        {
            "name": "Launch Server (production)",
            "cwd": "${workspaceRoot}/packages/qg_server_starter",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceRoot}/packages/qg_server_starter/bin/server.dart"
        },
        {
            "name": "Launch App (development)",
            "cwd": "${workspaceRoot}/packages/qg_flutter_starter",
            "request": "launch",
            "type": "dart",
            "program": "lib/bootstrap/main_development.dart",
            "args": [
              "--flavor",
              "development",
              "--target",
              "lib/bootstrap/main_development.dart"
            ]
          },
          {
            "name": "Launch App (staging)",
            "request": "launch",
            "type": "dart",
            "program": "lib/bootstrap/main_staging.dart",
            "args": [
                "--flavor",
                "staging",
                "--target",
                "lib/main_staging.dart"
              ]
          },
          {
            "name": "Launch App (production)",
            "request": "launch",
            "type": "dart",
            "program": "lib/bootstrap/main_production.dart",
            "args": [
                "--flavor",
                "production",
                "--target",
                "lib/main_production.dart"
            ],
            "flutterMode": "profile"
          }
    ],
    "compounds": [
        {
            "name": "Launch System (development)",
            "configurations": ["Launch App (development)", "Launch Server (development)"],
        },
        {
            "name": "Launch System (staging)",
            "configurations": ["Launch App (staging)", "Launch Server (staging)"],
        },
        {
            "name": "Launch System (production)",
            "configurations": ["Launch App (production)", "Launch Server (production)"],
        }
      ]
}
